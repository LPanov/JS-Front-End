function NxN(n) {
    let matrix = new Array(n);

    for (let i = 0; i < n; i++) {
        matrix[i] = new Array(n); 
        for (let j = 0; j < n; j++) {
            matrix[i][j] = n;
        }
    }

    for (let i = 0; i < n; i++) {
        console.log(matrix[i].join(' '));
    }
}

NxN(3);
NxN(7);